    <body>
        <div data-role="page" id="settingPage" data-theme="b">       
            
            <div data-role="header">
                <h1>Boresighter</h1>
            </div>

            <div id="settingMain" data-role="main" class="ui-content">                  
                    <label for="caliber">&nbsp;&nbsp;Caliber</label>                 
                    <select name="caliber" id="caliber">
                        <option value="c22lr">22LR</option>
                        <option value="c223remington">223 Remington</option>
                        <option value="c243win">243 Winchester</option>
                        <option value="c65sweden">6.5X55 Swedish</option>
                        <option value="c270win">270 Winchester</option>
                        <option value="c308win">308 Winchester</option>
                        <option value="c30-06">30-06 Springfield</option>
                        <option value="c300blk">300 ACC Blackout - Subsonic</option>
                        <option value="c76239">7.62X39</option>
                        <option value="c338lapua">338 Lapua Magnum</option>
                    </select> 
                    <div class="ui-grid-a">
                          <div class="ui-block-a">
                               <label for="shootingDistance">&nbsp;&nbsp;Shooting Distance</label>
                          </div>
                          <div class="ui-block-b">
                            <select name="shootingDistance" id="shootingDistance">
                                <option value="15">15 yard</option>
                                <option value="25">25 yard</option>
                                <option value="50">50 yard</option>         
                                <option value="50">100 yard</option>          
                            </select> 
                          </div>
                    </div>               
                    <div class="ui-grid-a">
                          <div class="ui-block-a">
                                <label for="distance">&nbsp;&nbsp;Zero Distance</label>  
                          </div>
                          <div class="ui-block-b">
                            <select name="distance" id="distance">
                                <option value="50">50 yard</option>
                                <option value="100">100 yard</option>
                                <option value="150">150 yard</option>
                                <option value="200">200 yard</option>
                                <option value="300">300 yard</option>
                            </select>
                          </div>
                    </div>                                  
                    
                     <div class="ui-grid-a">
                          <div class="ui-block-a">
                                <label for="clickUnit">&nbsp;&nbsp;Scope Adjustment</label>
                          </div>
                          <div class="ui-block-b">
                             <select name="clickUnit" id="clickUnit">
                                <option value="2">1/4 MOA</option>
                                <option value="1">1/8 MOA</option>                                                       
                                <option value="3">1/2 MOA</option>
                                <option value="4">1/10 MIL</option>
                            </select> 
                          </div>
                    </div>             

                    <label for="dx">&nbsp;&nbsp;Point of Impact (inch)</label>
                    <div class="ui-grid-a">
                          <div class="ui-block-a">
                                <input type="number" name="dx" id="dx"> 
                          </div>
                          <div class="ui-block-b">&nbsp;&nbsp;
                                <select name="xd" id="xd" data-role="slider">
                                    <option value="L">Left</option>
                                    <option value="R">Right</option>
                                </select> 
                          </div>
                    </div>                  
                    <div class="ui-grid-a">
                          <div class="ui-block-a">
                                <input type="number" name="dy" id="dy"> 
                          </div>
                          <div class="ui-block-b">&nbsp;&nbsp;
                                <select name="yd" id="yd" data-role="slider">
                                    <option value="H">High</option>
                                    <option value="L">Low</option>
                                </select> 
                          </div>
                    </div>    
                    <a href="#" data-role="button" class="ui-btn ui-icon-forward ui-btn-icon-left"  id="btnCalInch">Get Scope Adjustment</a>
             </div>

           <div data-role="footer" style="text-align:center;" data-position="fixed" data-fullscreen="true">
